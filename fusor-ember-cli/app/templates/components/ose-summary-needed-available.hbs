<label class="class">
  {{label}}
</label>

<div class="progress">
  <div class={{progressBarClass}} role="progressbar" aria-valuemin="0" aria-valuemax="100" aria-valuenow={{percentProgressInt}} style="width: {{styleWidth}};">
    <div style='float: left'>{{needed}} {{suffix}} </div>
  </div>
  <div style='float: right; color: #fff;'>{{available}} {{suffix}} </div>
</div>
